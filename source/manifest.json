{
  "manifest_version": 2,


  "name": "Radio Wave",
  "version": "3.0.0",
  "description": "A simple application for listening to online radio.",


  "icons": {
    "16": "icons/16.png",
    "24": "icons/24.png",
    "32": "icons/32.png",
    "48": "icons/64.png",
    "64": "icons/64.png",
    "96": "icons/128.png",
    "128": "icons/128.png",
    "256": "icons/256.png"
 },


 "background": {
    "page": "./background/index.html"
  },


  "browser_action": {
    "default_popup": "./popup/index.html",
    "default_icon": {
      "16": "icons/16.png",
      "24": "icons/24.png",
      "32": "icons/32.png",
      "48": "icons/64.png",
      "64": "icons/64.png",
      "96": "icons/128.png",
      "128": "icons/128.png",
      "256": "icons/256.png"
    }
  },


  "options_ui": {
    "page": "./options/index.html",
    "chrome_style": true,
    "open_in_tab": true
  }


